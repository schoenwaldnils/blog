{
  "presets": [
    "next/babel",
    "stage-0",
    "env"
  ],
  "plugins": [
    "inline-react-svg",
    [ "wrap-in-js", {
      "extensions": [
        ".css$"
      ]
    }],
    [ "styled-components", {
      "ssr": true,
      "displayName": true,
      "preprocess": false,
    }],
    [ "transform-define", "./env-config.js" ]
  ]
}
