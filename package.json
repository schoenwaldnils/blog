{
  "name": "blog",
  "version": "2.0.0",
  "description": "Nils Blog",
  "main": "index.js",
  "scripts": {
    "next": "next dev",
    "dev": "babel-node server.js",
    "build": "next build",
    "export": "NODE_ENV=production next build && next export",
    "start": "npm run dev",
    "test": "npm run lint",
    "lint": "npm run lint:css && npm run lint:js && npm run lint:html",
    "lint:css": "stylelint \"source/**/*.css\" --formatter verbose",
    "lint:css:fix": "stylelint \"source/**/*.css\" --formatter verbose --fix",
    "lint:js": "eslint source/.",
    "lint:js:fix": "eslint source/. --fix",
    "lint:html": "htmlhint out",
    "minify-html": "html-minifier --input-dir out --output-dir out --file-ext html --minify-css",
    "favicons-generate": "real-favicon generate faviconDescription.json faviconData.json static/assets/images/favicons",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:js:fix",
      "git add"
    ],
    "*.css": [
      "npm run lint:css:fix",
      "git add"
    ]
  },
