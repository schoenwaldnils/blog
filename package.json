{
  "name": "blog",
  "version": "2.0.0",
  "description": "Nils Blog",
  "main": "index.js",
  "scripts": {
    "start": "dotenv-load next dev",
    "build": "dotenv-load next build",
    "export": "NODE_ENV=production dotenv-load next build && dotenv-load next export",
    "test": "npm run lint",
    "lint": "npm run lint:css && npm run lint:sc && npm run lint:js",
    "lint:css": "stylelint \"**/*.css\" --formatter verbose",
    "lint:css:fix": "stylelint \"**/*.css\" --formatter verbose --fix",
    "lint:sc": "stylelint \"**/*.js\" --formatter verbose --config .stylelintrc-sc",
    "lint:js": "eslint \"**/*.js\"",
    "lint:js:fix": "eslint \"**/*.js\" --fix",
    "lint:html": "htmlhint out",
    "psi-pending": "babel-node .infrastructure/psi_tests_pending.js",
    "psi-test": "babel-node .infrastructure/psi_tests.js",
    "minify-html": "html-minifier --input-dir out --output-dir out --file-ext html --minify-css",
    "favicons-generate": "real-favicon generate faviconDescription.json faviconData.json static/assets/images/favicons"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "stylelint --formatter verbose --config .stylelintrc-sc",
      "git add"
    ],
    "**/*.css": [
      "stylelint --formatter verbose",
      "git add"
    ]
  },
  "keywords": [
    "next.js",
    "nextjs",
    "static"
  ],
  "repository": "github:schoenwaldnils/blog",
  "author": {
    "name": "Nils Sch√∂nwald",
    "email": "nils@schoenwald.media"
  },
  "license": "ISC",
  "dependencies": {
    "@babel/core": "7.1.2",
    "@babel/node": "7.0.0",
    "@babel/plugin-transform-modules-commonjs": "7.1.0",
    "@babel/register": "7.0.0",
    "@babel/traverse": "7.1.4",
    "@zeit/next-css": "1.0.1",
    "autoprefixer": "9.3.0",
    "babel-plugin-inline-react-svg": "1.0.1",
    "babel-plugin-styled-components": "1.8.0",
    "babel-plugin-transform-define": "1.3.0",
    "browserslist": "4.3.4",
    "classnames": "2.2.5",
    "contentful": "7.0.5",
    "contentful-management": "5.3.6",
    "css-loader": "1.0.1",
    "date-fns": "1.29.0",
    "debounce-promise": "3.1.0",
    "dotenv-load": "1.1.0",
    "dotenv-webpack": "1.5.7",
    "eslint": "5.4.0",
    "eslint-config-schoenwaldnils": "1.0.2",
    "express": "4.16.3",
    "favicons-webpack-plugin": "0.0.9",
    "he": "1.2.0",
    "highlight.js": "9.12.0",
    "html-minifier": "3.5.21",
    "html-react-parser": "0.4.7",
    "html-webpack-plugin": "3.2.0",
    "htmlhint": "0.10.0",
    "husky": "1.1.1",
    "isomorphic-fetch": "2.2.1",
    "lint-staged": "8.0.1",
    "marked": "0.5.1",
    "md5": "2.2.1",
    "next": "7.0.2",
    "normalize.css": "8.0.0",
    "octonode": "0.9.5",
    "postcss-calc": "7.0.1",
    "postcss-custom-media": "7.0.7",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-nested": "4.1.0",
    "postcss-pseudoelements": "5.0.0",
    "postcss-reporter": "6.0.0",
    "postcss-url": "8.0.0",
    "prop-types": "15.6.2",
    "psi": "3.1.0",
    "pygmentize-bundled": "2.3.0",
    "qs": "6.5.2",
    "raw-loader": "0.5.1",
    "react": "16.6.0",
    "react-dom": "16.6.0",
    "react-icons": "3.2.2",
    "react-tabs": "2.2.2",
    "style-loader": "0.23.1",
    "styled-components": "4.0.3",
    "stylelint": "9.7.1",
    "stylelint-config-schoenwaldnils": "2.1.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.5.0",
    "suitcss-base": "5.0.0",
    "url": "0.11.0",
    "webpack": "4.23.0",
    "yaml-front-matter": "4.0.0"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "cli-real-favicon": "0.0.6",
    "enzyme": "3.7.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
