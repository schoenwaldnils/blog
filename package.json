{
  "name": "blog",
  "version": "2.0.0",
  "description": "Nils Blog",
  "main": "index.js",
  "scripts": {
    "next": "next dev",
    "dev": "babel-node server.js",
    "build": "next build",
    "export": "NODE_ENV=production next build && next export",
    "start": "npm run dev",
    "test": "npm run lint",
    "lint": "npm run lint:css && npm run lint:sc && npm run lint:js",
    "lint:css": "stylelint \"**/*.css\" --formatter verbose",
    "lint:css:fix": "stylelint \"**/*.css\" --formatter verbose --fix",
    "lint:sc": "stylelint \"**/*.js\" --formatter verbose --config .stylelintrc-sc",
    "lint:js": "eslint \"**/*.js\"",
    "lint:js:fix": "eslint \"**/*.js\" --fix",
    "lint:html": "htmlhint out",
    "psi-pending": "babel-node .infrastructure/psi_tests_pending.js",
    "psi-test": "babel-node .infrastructure/psi_tests.js",
    "minify-html": "html-minifier --input-dir out --output-dir out --file-ext html --minify-css",
    "favicons-generate": "real-favicon generate faviconDescription.json faviconData.json static/assets/images/favicons",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "**/*.js": [
      "npm run lint:js:fix",
      "npm run lint:sc",
      "git add"
    ],
    "**/*.css": [
      "npm run lint:css",
      "git add"
    ]
  },
  "keywords": [],
  "author": {
    "name": "Nils Sch√∂nwald",
    "email": "nils@schoenwald.media"
  },
  "license": "ISC",
